    "dotenv": "^16.5.0",
    "gsap": "^3.12.5",
    "highlight.js": "11.9.0",
    "lucide": "^0.503.0",
    "lucide-svelte": "^0.510.0",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.10.1",
    "stripe": "^18.2.1",
    "studio": "^0.13.5",
    "supabase": "2.22.12",
    "superforms": "^0.0.1"
  }
}
