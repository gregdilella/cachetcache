{
  "name": "portfolio-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "bun run test:integration && bun run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "deploy": "bun run build && bun run deploy:cloudflare",
    "deploy:cloudflare": "wrangler pages deploy .svelte-kit/cloudflare",
    "deploy:vercel": "vercel deploy --prod",
    "deploy:advanced": "bun deploy.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@internationalized/date": "^3.8.0",
    "@playwright/test": "^1.49.1",
    "@skeletonlabs/skeleton": "2.7.0",
    "@skeletonlabs/tw-plugin": "0.3.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.11.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.16",
    "bits-ui": "0.22.0",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "formsnap": "1.0.1",
    "globals": "^15.15.0",
    "mdsvex": "^0.12.5",
    "postcss": "8.4.33",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "sveltekit-rate-limiter": "^0.5.2",
    "sveltekit-superforms": "^2.27.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-motion": "^1.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-tailwind-purgecss": "0.2.0",
    "vitest": "^1.6.0",
    "wrangler": "^4.20.1",
    "zod": "^3.24.1"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.830.0",
    "@aws-sdk/s3-request-presigner": "^3.830.0",
    "@emailjs/browser": "^3.12.1",
    "@floating-ui/dom": "1.5.3",
    "@melt-ui/svelte": "^0.86.6",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@sveltejs/adapter-cloudflare": "^7.0.1",
    "@sveltejs/enhanced-img": "^0.2.1",
